{
  "name": "@builderhub/prettier-config",
  "version": "0.0.7",
  "description": "Builderhub prettier config",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "release": "standard-version --no-verify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/builderhub-platform/prettier-config.git"
  },
  "keywords": [
    "prettier",
    "prettier-config",
    "builderhub"
  ],
  "author": {
    "name": "Eunchurn Park",
    "email": "eunchurn@builderhub.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/builderhub-platform/prettier-config/issues"
  },
  "homepage": "https://github.com/builderhub-platform/prettier-config#readme",
  "dependencies": {
    "prettier": "2.8.2"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@types/prettier": "2.7.2",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.2",
    "pretty-quick": "3.1.3",
    "standard-version": "9.5.0",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "prettier": "^2.8.2"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
